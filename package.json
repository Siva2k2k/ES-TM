{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "serve -s dist -l $PORT",
    "test:unit": "vitest run __tests__/unit",
    "test:component": "vitest run __tests__/component --pool=threads --poolOptions.threads.singleThread=true",
    "test:component:memory": "node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run __tests__/component",
    "test:e2e": "playwright test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.5",
    "vite": "^5.4.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.0",
    "@supabase/supabase-js": "^2.57.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "dotenv": "^16.6.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "faker": "^5.5.3",
    "globals": "^15.9.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.20",
    "vitest": "^3.2.4"
  }
}
